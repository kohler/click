%info
Basic RatedUnqueue test

%script
click --simtime CONFIG

%file CONFIG
InfiniteSource()
	-> Queue(10)
	-> u1 :: RatedUnqueue(RATE 2)
	-> c1 :: Counter
	-> Discard;

InfiniteSource()
	-> Queue(10)
	-> u2 :: RatedUnqueue(RATE 2, BURST_SIZE 50)
	-> c2 :: Counter
	-> Discard;

InfiniteSource()
	-> Queue(10)
	-> u3 :: RatedUnqueue(RATE 2, BURST_DURATION 25)
	-> c3 :: Counter
	-> Discard;

InfiniteSource()
	-> Queue(10)
	-> u4 :: RatedUnqueue(RATE 100, BURST_SIZE 1)
	-> c4 :: Counter
	-> Discard;

Script(wait 10, read c1.count, read c2.count, read c3.count, read c4.count, write stop);

%expect stdout
%expect -w stderr
c1.count:
20
c2.count:
69
c3.count:
69
c4.count:
{{909|910}} {{?this is because of inefficiencies with BURST_SIZE == 1 + simtime not being perfect}}
