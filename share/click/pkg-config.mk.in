CLICKLINUX_FIXINCLUDES_PROGRAM = @CLICKLINUX_FIXINCLUDES_PROGRAM@

CLICKCPPFLAGS = @CPPFLAGS@
CLICKCFLAGS = @CFLAGS@
CLICKKERNEL_CFLAGS = @KERNEL_CFLAGS@
