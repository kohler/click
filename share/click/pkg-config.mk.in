CLICKDEFS = @DEFS@
CLICKINCLUDES = -I$(clickbuild_includedir) -I$(clickbuild_srcdir) @PCAP_INCLUDES@
CLICKLDFLAGS = @LDFLAGS@
CLICKLDMODULEFLAGS = @LDMODULEFLAGS@

CLICKAUTOCONF = @AUTOCONF@
CLICKGMAKE = @GMAKE@
CLICKINSTALL = @CLICKINSTALL@
@SUBMAKE@
